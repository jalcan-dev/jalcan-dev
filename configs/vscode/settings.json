{
  "window.zoomLevel": -1,
  "security.workspace.trust.untrustedFiles": "open",
  "git.ignoreMissingGitWarning": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "codeium.enableConfig": {
    "*": true,
    "xml": true,
    "plaintext": true
  },
  "codeium.indexingRetentionPeriod": 10,
  "editor.formatOnSave": true,
  "explorer.autoReveal": "focusNoScroll",
  "files.associations": {
    "*.icd": "json",
    "*.scd": "json",
    "*.lst": "json",
    "*.snc": "json"
  },
  "workbench.colorTheme": "GitHub Dark",
  "liveServer.settings.donotShowInfoMsg": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "codeium.disableSupercomplete": true,
  "codeium.enableInComments": false,
  "editor.lineNumbers": "on",
  "chat.sendElementsToChat.attachCSS": false,
  "chat.sendElementsToChat.attachImages": false,
  "chat.sendElementsToChat.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.openRepositoryInParentFolders": "never",
  "remote.autoForwardPortsSource": "hybrid",
  "C_Cpp.copilotHover": "disabled",
  "github.copilot.advanced": {},
  "github.copilot.chat.agent.autoFix": false,
  "diffEditor.maxComputationTime": 0,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}
